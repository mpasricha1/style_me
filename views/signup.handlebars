<form class="signup">
    <div class="container">
        <h1>Sign Up</h1>
        <p>Please fill in this form to create an account.</p>
        <hr />
        <label for="first_name"><b>First Name</b></label>
        <input type="text" id="first_name" placeholder="First Name" name="first_name" required />

        <label for="last_name"><b>Last Name</b></label>
        <input type="text" id="last_name" placeholder="Last Name" id="last_name" required />
    </div>

    <div class="container">
        <label for="email"><b>Email</b></label>
        <input type="text" id="email-field" placeholder="Email" name="email" required />

        <label for="psw"><b>Password</b></label>
        <input type="password" id="psw-field" placeholder="Enter Password" name="psw" required />
    </div>

  <div class="btn_container">
     <button type="submit" class="signupbtn">Sign Up</button>
     {{!-- <button type="button" class="cancelbtn" style="background-color: #F44336">Cancel</button> --}}
     <a href="/" class="cancelBtn"  style="background-color: #F44336"role="button">Cancel</a>
    </div>
</form>
